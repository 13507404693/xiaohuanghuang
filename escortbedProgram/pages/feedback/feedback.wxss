/* pages/feedback/feedback.wxss */
.banner-w{
  overflow: hidden;
}
.banner{
  margin: 24rpx 24rpx 0;
}
.banner image{
  width: 100%;
  display: block;
}
.gray-w{
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  background: #f2f2f2;
  border-radius: 20rpx;
  margin: 24rpx;
  align-items: center;
  padding: 0 30rpx;
}
.gray-w .head{
  font-size: 26rpx;
  height: 26rpx;
  line-height: 26rpx;
  color: #212121;
  flex: 1;
}
.gray-w .foot{
  display: block;
}
.gray-w .icon-scan{
  width: 40rpx;
}
.gray-w .icon-right{
  width: 16rpx;
}
.title{
  font-size: 28rpx;
  color: #212121;
  margin: 40rpx 20rpx 24rpx;
}
.title.must::before{
  content: "*";
  color: #ea5810;
}
.type-list{
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.type-list .type-name{
  font-size: 26rpx;
  text-align: center;
  color: #212121;
  margin-top: 10rpx;
}
.type-list .type{
  width: 110rpx;
  height: 110rpx;
  border-radius: 100%;
  border: 1rpx solid #d1d1d1;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.type-list .active .type{
  border: 1rpx solid #23ac38;
  background: #f8fff7;
}
.type-list .active .type-name{
  color: #23ac38;
}
.type-list .type image{
  display: block;
  width: 46rpx;
}
.memo{
  margin: 24rpx;
}
.memo textarea{
  display: block;
  background: #f2f2f2;
  width: 100%;
  font-size: 26rpx;
  color: #212121;
  padding: 20rpx;
  box-sizing: border-box;
}
.photo-has{
  position: relative;
  margin: 24rpx;
}
.photo-has .photo-del{
  position: absolute;
  width: 40rpx;
  height: 40rpx;
  border-radius: 100%;
  background: #ea5810;
  top: -8rpx;
  left: 100rpx;
  box-shadow: 0 0 10rpx #999;
  transform: rotate(45deg);
}
.photo-has .photo-del::before{
  content: "";
  display: block;
  width: 2rpx;
  height: 20rpx;
  background: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -10rpx;
  margin-left: -1rpx;
}
.photo-has .photo-del::after{
  content: "";
  display: block;
  width: 20rpx;
  height: 2rpx;
  background: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -1rpx;
  margin-left: -10rpx;
}
.photo-has image{
  width: 130rpx;
  height: 130rpx;
  display: block;
  border-radius: 10rpx;
  border: 1rpx solid #fff;
}
.photo{
  display: flex;
  align-items: center;
  margin: 24rpx;
  font-size: 26rpx;
  color: #212121;
}
.photo .icon-add{
  width: 130rpx;
  height: 130rpx;
  border: 1rpx dashed #d1d1d1;
  border-radius: 10rpx;
  margin-right: 30rpx;
  position: relative;
}
.photo .icon-add::after{
  content: "";
  display: block;
  position: absolute;
  width: 8rpx;
  height: 56rpx;
  border-radius: 8rpx;
  background: #666;
  top: 50%;
  left: 50%;
  margin-top: -28rpx;
  margin-left: -4rpx;
}
.photo .icon-add::before{
  content: "";
  display: block;
  position: absolute;
  height: 8rpx;
  width: 56rpx;
  border-radius: 8rpx;
  background: #666;
  top: 50%;
  left: 50%;
  margin-left: -28rpx;
  margin-top: -4rpx;
}
.btn-w{
  padding: 24rpx 24rpx 48rpx;
}
.btn-margin{
  margin-top: 100rpx;
}

.mack{
  position:fixed;
  z-index: 999;
  top:0px;
  left:0px;
  width: 100%;
  height: 100%;
  background:#fff;
  display:block!important;
}

.w_120{width:120rpx;}
.mackbg{background:#f2f2f2;padding:20rpx 20rpx;}
.img_wh16{width:14rpx;height:16rpx;transform: rotate(180deg);}
.img_wh16s{width:14rpx;height:16rpx;}
.f_30{font-size:30rpx;}
.pr_top10{position:relative;top:10rpx;}
.pd_lefe10{padding-left:16rpx;}
.pd_20{padding:20rpx;border-bottom:1px solid #eee;}
.c_99{color:#666}
.hiden{overflow:hidden}