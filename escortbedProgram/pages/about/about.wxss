/* pages/about/about.wxss */
page{
  background: #f5f5f5;
  position: relative;
  min-height: 100vh;
  padding-bottom: 120rpx;
  box-sizing: border-box;
}
.banner-w{
  overflow: hidden;
}
.banner{
  margin: 24rpx 24rpx 40rpx;
}
.banner image{
  width: 100%;
  display: block;
}
.cells{
  font-size: 30rpx;
  background: #fff;
}
.cells .cell{
  margin: 0 24rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #d1d1d1;
}
.cells .cell:last-child{
  border: none;
}
.cells .cell .cell-hd{
  width: 180rpx;
  color: #212121;
}
.cells .cell .cell-bd{
  flex: 1;
  color: #666;
}
.footer{
  position: absolute;
  text-align: center;
  width: 100vw;
  font-size: 24rpx;
  color: #999;
  bottom: 20rpx;
}